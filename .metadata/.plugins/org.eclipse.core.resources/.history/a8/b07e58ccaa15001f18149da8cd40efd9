package chap18.sec10;

import java.io.Serializable;

public class Product implements Serializable {

	private String name;
	private String price;
	
	public Product(String name, String price) {
		this.name = name;
		this.price = price;
	}
	
	@Override
	public String toString() {
		return name + ":" + price;}
	
}
