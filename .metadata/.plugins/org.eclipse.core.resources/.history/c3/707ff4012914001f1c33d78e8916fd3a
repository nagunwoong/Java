package chap18.sec10;

import java.io.FileOutputStream;
import java.io.ObjectInputStream;

import chap013.sec02.exam01.Product;

public class ObjectInputOutPutStreamExmaple {

	public static void main(String[] args) {
		
		//다양한 객체를 파일에 저장하고 다시 읽어 객체로 복원
		// FileOutputStream 에 ObjectInputOutputStream 를 
		FileOutputStream fos = new FileOutputStream("C:/Temp/object.dat");
		ObjectInputStream oos = new ObjectInputStream(fos);
		
		// 객체 생성
		Member m1 = new Member("fall", "단품이");
		Product p1 = new Product("노트북", 1500000);
		int[] arr1 = {1,2,3};
		
		oos.writeObject(m1);
		oos.writeObject(p1);
		oos.writeObject(arr1);
		
		oos.flush(); oos.close(); fos.close();
		
		//객체를 직렬화한 
		
		Member m2 = (Member)
	
		
		
	}

}
